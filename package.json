{
  "name": "cheer",
  "version": "0.0.1",
  "description": ":clap: Galvanize your Markdown files with dynamic content",
  "keywords": [
    "template"
  ],
  "homepage": "https://github.com/aymericbeaumet/cheer",
  "bugs": "https://github.com/aymericbeaumet/cheer/issues",
  "license": "CC0-1.0",
  "author": "Aymeric Beaumet <hi@aymericbeaumet.com> (https://aymericbeaumet.com)",
  "files": "lib",
  "main": "lib/index.js",
  "bin": "lib/cli.js",
  "repository": "https://github.com/aymericbeaumet/cheer.git",
  "scripts": {
    "test": "xo && jest && npm run cheer -- --lint readme.md",
    "prepublish": "babel src --out-dir=lib",
    "dev": "jest --watch",
    "cheer": "npm run prepublish && node ./lib/cli.js"
  },
  "dependencies": {
    "babel-core": "^6.21.0",
    "babel-types": "^6.21.0",
    "bluebird": "^3.4.6",
    "documentation": "^3.0.4",
    "execa": "^0.6.3",
    "get-stream": "^3.0.0",
    "js-yaml": "^3.7.0",
    "json5": "^0.5.1",
    "lodash": "^4.17.2",
    "meow": "^3.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-origin": "^1.4.0",
    "jest-cli": "^19.0.2",
    "mock-fs": "^4.3.0",
    "nock": "^9.0.2",
    "xo": "^0.18.1"
  },
  "xo": {
    "parser": "babel-eslint",
    "space": 2,
    "semicolon": false,
    "esnext": true,
    "envs": [
      "node",
      "jest"
    ]
  },
  "jest": {
    "coverageDirectory": "coverage",
    "testRegex": "/test/.*"
  },
  "babel": {
    "extends": "babel-origin"
  }
}
