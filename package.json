{
  "name": "cheer",
  "version": "0.0.1",
  "description": "Galvanize your markdown files",
  "main": "lib/index.js",
  "bin": "lib/cli.js",
  "scripts": {
    "test": "eslint . && jest",
    "prepublish": "babel src --out-dir=lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aymericbeaumet/cheer.git"
  },
  "keywords": [],
  "author": "Aymeric Beaumet <hi@aymericbeaumet.com> (https://aymericbeaumet.com)",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/aymericbeaumet/cheer/issues"
  },
  "homepage": "https://github.com/aymericbeaumet/cheer",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-jest": "^15.0.0",
    "babel-plugin-transform-async-to-module-method": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-preset-stage-2": "6.13.0",
    "eslint": "^3.4.0",
    "eslint-config-opinionated": "1.4.1",
    "jest-cli": "^15.1.1"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "bluebird": "^3.4.6",
    "lodash": "^4.15.0",
    "meow": "^3.7.0"
  },
  "eslintConfig": {
    "extends": [
      "opinionated/node",
      "opinionated/fp"
    ]
  },
  "babel": {
    "plugins": [
      [
        "transform-async-to-module-method",
        {
          "module": "bluebird",
          "method": "coroutine"
        }
      ],
      "transform-runtime"
    ],
    "presets": [
      "stage-2",
      "es2015-node4"
    ],
    "sourceMaps": "inline"
  }
}
